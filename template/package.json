{
  "name": "HelloWorld",
  "description": "An opinionated template to bootstrap your next React Native app with all the time-wasting packages you need to have.",
  "license": "MIT",
  "main": "index.js",
  "author": "gabimoncha@gmail.com",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "./jest.sh",
    "bundle": "react-native-bundle-visualizer",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "postinstall": "patch-package",
    "detox:ios": "./detox.sh ios",
    "detox:android": "./detox.sh android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.15.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "6.0.0",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@sentry/react-native": "2.4.0",
    "@welldone-software/why-did-you-render": "6.1.1",
    "code-push": "4.0.2",
    "i18next": "19.9.2",
    "react": "17.0.1",
    "react-i18next": "11.8.12",
    "react-native": "0.64.0",
    "react-native-appstate-hook": "1.0.5",
    "react-native-code-push": "7.0.0",
    "react-native-crisp-chat-sdk": "0.6.0",
    "react-native-dotenv": "2.5.3",
    "react-native-fast-image": "8.3.4",
    "react-native-file-logger": "0.2.1",
    "react-native-gesture-handler": "1.10.3",
    "react-native-keychain": "6.2.0",
    "react-native-notifications": "3.4.2",
    "react-native-permissions": "3.0.1",
    "react-native-reanimated": "1.13.3",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "2.18.1",
    "react-native-startup-time": "1.3.0",
    "react-native-svg": "12.1.0",
    "react-native-svg-transformer": "0.14.3",
    "react-native-vector-icons": "8.1.0",
    "react-native-webview": "11.3.2",
    "react-query": "3.13.4",
    "styled-components": "5.2.3",
    "styled-system": "5.1.5",
    "zustand": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/runtime": "^7.13.9",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.25",
    "@storybook/react-native": "5.3.25",
    "@testing-library/jest-native": "3.4.3",
    "@testing-library/react-hooks": "5.1.1",
    "@testing-library/react-native": "7.2.0",
    "@types/detox": "17.14.0",
    "@types/jest": "26.0.22",
    "@types/react-native": "0.64.12",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components-react-native": "5.1.1",
    "@types/styled-system": "5.1.10",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-styled-components": "1.12.0",
    "detox": "18.9.0",
    "eslint": "7.23.0",
    "eslint-plugin-detox": "1.0.0",
    "eslint-plugin-jest": "24.3.2",
    "husky": "5.2.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "lint-staged": "10.5.4",
    "metro-react-native-babel-preset": "0.65.2",
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.2.1",
    "react-dom": "17.0.1",
    "react-native-bundle-visualizer": "2.2.1",
    "react-test-renderer": "17.0.1",
    "typescript": "4.2.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./jest.setup.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./node_modules/react-native/jest/setup.js",
      "./node_modules/react-native/Libraries/Utilities/__mocks__/BackHandler.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "/assetsTransformer.js",
      "\\.svg": "<rootDir>/__mocks__/svgMocks.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|@react-navigation)"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint . --fix",
      "prettier --write .",
      "jest --bail --findRelatedTests"
    ]
  }
}
